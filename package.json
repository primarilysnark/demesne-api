{
  "name": "@primarilysnark/demesne-api",
  "version": "1.0.0",
  "description": "Companion API to Demesne",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev:watch": "npm start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:watch",
    "format:check": "prettier -c './{src,src}/**/*.{ts,tsx}'",
    "format": "prettier --write './{src,src}/**/*.{ts,tsx}'",
    "lint": "tslint -c tslint.json --fix '{src,tools}/**/*.{tsx,ts}'",
    "predev:watch": "npm run build",
    "start": "node .",
    "tools:nethys-legacy-parser:build": "tsc --build ./tools/nethys-legacy-parser/tsconfig.json",
    "pretools:nethys-legacy-parser:start": "npm run tools:nethys-legacy-parser:build",
    "tools:nethys-legacy-parser:start": "node ./tools/nethys-legacy-parser/dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/primarilysnark/demesne-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@primarilysnark/sequelize-json-api": "^0.2.1",
    "body-parser": "^1.19.0",
    "consola": "^2.11.3",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "jsdom": "^16.2.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.4",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/express-ws": "^3.0.0",
    "@types/jsdom": "^16.1.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.1",
    "@types/node-fetch": "^2.5.4",
    "@types/validator": "^12.0.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.5"
  }
}
